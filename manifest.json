{
  "manifest_version": 2,
  "name": "__MSG_name__",
  "version": "1.0.0",
  "description": "Move tabs to another window from right click menu on tabs.",
  "applications": {
    "gecko": {
      "strict_min_version": "53.0"
    }
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "move.js"
    ]
  },
  "default_locale": "en",
  "options_ui": {
    "page": "options.html"
  }
}
